{
  "name": "@amzn/kepler-media-types",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf node_modules buildinfo.json dist bvm build/* build",
    "lint": "eslint src tst --ext .ts,.tsx",
    "lint:fix": "eslint src tst --ext .ts,.tsx --fix",
    "docgen": "npx typedoc --basePath src/ -entryPointStrategy expand --excludePrivate --excludeProtected --excludeInternal --excludeNotDocumented --disableSources --hideGenerator src/ --out build/docs",
    "compile": "tsc -p tsconfig-build.json",
    "release": "npm run docgen",
    "prepack": "npm run compile"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@react-native/eslint-config": "0.72.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "eslint": "^8.12.0",
    "prettier": "^2.6.2",
    "typescript": "4.8.4",
    "typedoc": "^0.26.0"
  },
  "npm-pretty-much": {
    "runRelease": "always"
  }
}
{
  "name": "@amzn/keplerscript-turbomodule-api",
  "version": "1.2.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "dist",
    "LICENSE",
    "rn-codegen",
    "types",
    "THIRD_PARTY_NOTICES.txt"
  ],
  "scripts": {
    "test": "jest --silent",
    "reset-test-snaps": "npx . codegen __tests__/modules/js/SampleTurboModule.ts --outputPath __tests__/modules/kepler --namespace com::amazon::test --new",
    "lint": "eslint src --ext .ts,.tsx && eslint tst --ext .ts,.tsx",
    "autofix": "eslint --fix src --ext .ts,.tsx && eslint --fix tst --ext .ts,.tsx",
    "format": "prettier --write '**/*.{md,json}' '**/*.{js,jsx,ts,tsx}' '!.tmp/**'",
    "compile": "tsc",
    "prepack": "npm-run-all compile test"
  },
  "dependencies": {
    "@babel/parser": "^7.20.0",
    "flow-parser": "^0.206.0",
    "invariant": "^2.2.4",
    "jscodeshift": "^0.14.0",
    "nullthrows": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./dist/**/*.js"
    ],
    "testMatch": [
      "**/__tests__/*.test.js"
    ]
  }
}